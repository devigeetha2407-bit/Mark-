#include <stdio.h>

int main() {
    int n;
    int marks[50], temp;

    // Input number of students
    printf("Enter number of students: ");
    scanf("%d", &n);

    // Input marks
    for(int i = 0; i < n; i++) {
        printf("Enter marks of student %d: ", i + 1);
        scanf("%d", &marks[i]);
    }

    // Sorting using simple bubble sort
    for(int i = 0; i < n - 1; i++) {
        for(int j = i + 1; j < n; j++) {
            if(marks[i] > marks[j]) {
                temp = marks[i];
                marks[i] = marks[j];
                marks[j] = temp;
            }
        }
    }

    // Display sorted marks
    printf("\nMarks in ascending order:\n");
    for(int i = 0; i < n; i++) {
        printf("%d ", marks[i]);
    }

    return 0;
}
