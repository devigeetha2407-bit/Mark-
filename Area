#include <stdio.h>

// Function to calculate area of rectangle
float calculateArea(float length, float width) {
    return length * width;
}

int main() {
    float length, width, area;

    printf("Enter length: ");
    scanf("%f", &length);

    printf("Enter width: ");
    scanf("%f", &width);

    area = calculateArea(length, width); // Function call

    printf("Area of the rectangle = %.2f\n", area);

    return 0;
}
