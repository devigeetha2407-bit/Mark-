#include <stdio.h>

int main() {
    int n;

    // Ask the user for the total number of students
    printf("Enter the total number of students: ");
    scanf("%d", &n);

    // Print attendance numbers
    printf("Attendance numbers of students:\n");
    for(int i = 1; i <= n; i++) {
        printf("Student %d\n", i);
    }

    return 0;
}
