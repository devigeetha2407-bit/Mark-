#include <stdio.h>

int main(void) {
    int marks;

    // Input marks
    printf("Enter marks: ");
    scanf("%d", &marks);

    // Grade calculation
    if (marks >= 90 && marks <= 100) {
        printf("Grade: A\n");
    } else if (marks >= 75) {
        printf("Grade: B\n");
    } else if (marks >= 60) {
        printf("Grade: C\n");
    } else if (marks >= 40) {
        printf("Grade: D\n");
    } else if (marks >= 0) {
        printf("Grade: F\n");
    } else {
        printf("Invalid marks\n");
    }

    return 0;
}
